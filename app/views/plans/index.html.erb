<p id="notice"><%= notice %></p>

<h1>Plans</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Major id</th>
      <th>Catalog id</th>
      <th>Current year</th>
      <th>Current semester</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.Name %></td>
        <td><%= plan.User %></td>
        <td><%= plan.Major_ID %></td>
        <td><%= plan.Catalog_ID %></td>
        <td><%= plan.Current_Year %></td>
        <td><%= plan.Current_Semester %></td>
        <td><%= link_to 'Show', plan %></td>
        <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
        <td><%= link_to 'Destroy', plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plan', new_plan_path %>
